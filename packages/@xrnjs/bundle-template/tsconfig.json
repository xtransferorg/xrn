{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Xrn",
  
	"compilerOptions": {
	  "allowImportingTsExtensions": true,
	  "allowJs": true,
	  "esModuleInterop": true,
	  "jsx": "react-native",
	  "lib": ["dom", "esnext"],
	  "moduleResolution": "node",
	  "noEmit": true,
	  "resolveJsonModule": true,
	  "skipLibCheck": true,
	  "target": "ESNext",
	},
  
	"exclude": [
	  "node_modules",
	  "babel.config.js",
	  "metro.config.js",
	  "jest.config.js",
	  "android",
	  "ios"
	]
  }
  