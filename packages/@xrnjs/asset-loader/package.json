{
  "name": "@xrnjs/asset-loader",
  "version": "0.0.2",
  "description": "资源加载",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build",
    "prepare": "yarn clean && yarn build",
    "watch": "nodemon --ignore build/ --watch ./src -C -e js,jsx,ts,tsx,css,less --debug -x 'yalc publish --push'"
  },
  "files": [
    "src",
    "build",
    "android",
    "ios",
    "harmony/*.har",
    "harmony/xrn_asset_loader",
    "!harmony/xrn_asset_loader/oh_modules",
    "!harmony/xrn_asset_loader/build",
    "!harmony/xrn_asset_loader/.hvigor",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "keywords": [
    "react-native",
    "xrn",
    "xrn-asset-loader",
    "XRNAssetLoader",
    "xrnjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/xtransferorg/xrn",
  "bugs": {
    "url": "https://github.com/xtransferorg/xrn/issues"
  },
  "author": "",
  "license": "Apache-2.0",
  "homepage": "https://github.com/xtransferorg/xrn#readme",
  "devDependencies": {
    "@react-native-oh/react-native-harmony": "0.72.38",
    "@types/react": "^18.0.25",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.1",
    "react-native": "0.72.5",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@xrnjs/react-native-code-push": "*",
    "lodash": "*",
    "react": "*",
    "react-native": "*"
  },
  "harmony": {
    "codegenConfig": {
      "specPaths": [
        "./src"
      ]
    }
  },
  "xrnMeta": {
    "name": "XRNAssetLoader",
    "title": "My new module",
    "showName": "XRNAssetLoader",
    "group": "资源管理",
    "description": "My new module",
    "packageName": "xrn-asset-loader",
    "sdkPath": "resource-management/xrn-asset-loader"
  }
}
