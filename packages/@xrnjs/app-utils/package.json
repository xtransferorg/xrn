{
  "name": "@xrnjs/app-utils",
  "version": "0.0.2",
  "description": "提供常用的应用级工具函数，提升开发效率",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build",
    "prepare": "yarn clean && yarn build",
    "watch": "nodemon --ignore build/ --watch ./src -C -e js,jsx,ts,tsx,css,less --debug -x 'yalc publish --push'"
  },
  "files": [
    "src",
    "build",
    "android",
    "ios",
    "harmony/*.har",
    "harmony/xrn_app_utils",
    "!harmony/xrn_app_utils/oh_modules",
    "!harmony/xrn_app_utils/build",
    "!harmony/xrn_app_utils/.hvigor",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "keywords": [
    "react-native",
    "xrn",
    "xrn-app-utils",
    "XRNAppUtils",
    "xrnjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/xtransferorg/xrn",
  "bugs": {
    "url": "https://github.com/xtransferorg/xrn/issues"
  },
  "author": "",
  "license": "Apache-2.0",
  "homepage": "https://xtransferorg.github.io/xrn/versions/latest/sdk/tools/xrn-app-utils",
  "devDependencies": {
    "@react-native-oh/react-native-harmony": "0.72.38",
    "@types/react": "^18.0.25",
    "nodemon": "^3.1.1",
    "react-native": "0.72.5",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@xrnjs/modules-core": "0.0.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "harmony": {
    "codegenConfig": {
      "specPaths": [
        "./src"
      ]
    }
  },
  "xrnMeta": {
    "name": "XrnAppUtils",
    "title": "应用工具集",
    "showName": "应用工具集",
    "group": "工具函数",
    "description": "提供常用的应用级工具函数，提升开发效率",
    "packageName": "xrn-app-utils",
    "sdkPath": "tools/xrn-app-utils"
  }
}
